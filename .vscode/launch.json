    "version": "0.2.0",
    "configurations": [
        {
            "name": "GitWatcher: Graph",
            "type": "PowerShell",
            "request": "launch",
            "script": "./WatchGit.ps1",
            "args": ["${env:PATHARG}", "-GitCommand", "Graph"]
        },
        {
            "name": "GitWatcher: Status",
            "type": "PowerShell",
            "request": "launch",
            "script": "./WatchGit.ps1",
            "args": ["${env:PATHARG}", "-GitCommand", "Status"]
        },
        {
            "name": "GitWatcher: Branch",
            "type": "PowerShell",
            "request": "launch",
            "script": "./WatchGit.ps1",
            "args": ["${env:PATHARG}", "-GitCommand", "Branch"]
        },
        {
            "name": "GitWatcher: Start",
            "type": "PowerShell",
            "request": "launch",
            "script": "./StartGitWatcher.ps1",
            "args": ["${env:PATHARG}"]
        },
        {
            "name": "Install",
            "type": "PowerShell",
            "request": "launch",
            "script": "./Install.ps1"
        },
        {
            "name": "PowerShell: Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": ["${env:PATHARG}"]
        }
    ]
}